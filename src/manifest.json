{
    "manifest_version": 3,
    "name": "Cookie Auto Cleaner",
    "version": "1.0.0",
    "description": "Automatically cleans cookies at set intervals for enhanced privacy and performance.",
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "permissions": ["cookies", "storage", "alarms", "activeTab", "scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icons/icon48.png"
    }
    ,
    "host_permissions": ["<all_urls>"],
    "options_ui": {
      "page": "popup.html",
      "open_in_tab": false
    }
  }
  